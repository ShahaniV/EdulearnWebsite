@model IEnumerable<EdulearnWebsite.Models.Module>

@{
	ViewData["Title"] = "Edulearn | Kindergarten Modules";
}
<link rel="stylesheet" href="~/wwwroot/css/library.css" />

<link rel="stylesheet" href="https://cdn.rawgit.com/coverflowjs/coverflow/master/dist/coverflow.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
<link rel="stylesheet" href="~/wwwroot/css/modules.css" />


<!-- Wrapper -->
<div id="wrapper">
	<!-- Main -->
	<div id="main">
		<div class="inner">
			<!-- Header -->
			<header id="header">

				<!-- Home Back Button -->
				<a onclick="BackFunc()" class="bbutton">
					<div class="home-back-button">
						<i class="fa fa-arrow-left  fa-fw" aria-hidden="true"></i>Back
					</div>
				</a>

				<div class="main-cont">
					<div id="time-i">
						<h1>-</h1>
					</div>
					<div id="date-i">
						<h1>-</h1>
					</div>
					<div id="day-i">
						<h1>-</h1>
					</div>
				</div>
			</header>

			<!-- Content -->
			<section>
				<header class="main">
					<h2 style="margin: 0 auto !important; text-align: center">Kindergarten</h2>
					<p style="text-align: center">Modules from <a href="https://lrmds.deped.gov.ph/" target="_blank"> DEPED LEARNING PORTAL</a></p>
				</header>
				
				<ul class="faq-list" id="kindergarten">
					<header class="main">
						<h3>Kindergarten</h3>
						<div style="width: 100px; background-color:bisque;" class="text-center"> Quarter 1 </div>
						<div style="		width: 100px; background-color: #c4ecff;" class="text-center"> Quarter 2 </div>
					</header>
					<!-- kindergarten -->
					<li class="J_list">
						<div class="list-header">Kindergarten</div>
						<div class="list-content">
							<div class="list-content-inner">
								<h3 class="text-center">Math</h3>
								<div class="table-wrapper">
									<table style="text-align: center;">
										<thead>
											<tr>
												<th style="text-align: center;">Title</th>
												<th style="text-align: center;">Description</th>
												<th style="text-align: center;">Actions</th>
											</tr>
										</thead>
										<tbody>
											@*@foreach (var item in Model.Where(c => c.gradeLevel.Equals(1)))*@
											@foreach (var item in Model.Where(c => c.subject == "Math" && c.gradeLevel == "Kindergarten" && c.quarterNo == 1))
											{
												<tr>
													<td style="	background-color: bisque; color:#3d4449;" href="@(item.files)" target="_blank" class="pb-2">
														@Html.DisplayFor(modelItem => item.moduleNo)
														@Html.DisplayFor(modelItem => item.title)

													</td>
													<td>
														@Html.DisplayFor(modelItem => item.description)
													</td>

													<td>
														<a href="@(item.files)" target="_blank" class="button mt-2">View <i class="fa fa-book" aria-hidden="true"></i></a>
													</td>
												</tr>
											}
											@foreach (var item in Model.Where(c => c.subject == "Math" && c.gradeLevel == "Kindergarten" && c.quarterNo == 2))
											{
												<tr>
													<td style="		background-color: #c4ecff; color:#3d4449;" href="@(item.files)" target="_blank" class="pb-2">
														@Html.DisplayFor(modelItem => item.moduleNo)
														@Html.DisplayFor(modelItem => item.title)


													</td>
													<td>
														@Html.DisplayFor(modelItem => item.description)
													</td>

													<td>
														<a href="@(item.files)" target="_blank" class="button mt-2">View <i class="fa fa-book" aria-hidden="true"></i></a>
													</td>
												</tr>
											}

										</tbody>
									</table>
								</div>
								<h3 class="text-center">Filipino</h3>
								<div class="table-wrapper">
									<table style="text-align: center;">
										<thead>
											<tr>
												<th style="text-align: center;">Title</th>
												<th style="text-align: center;">Description</th>
												<th style="text-align: center;">Actions</th>
											</tr>
										</thead>
										<tbody>
											@*@foreach (var item in Model.Where(c => c.gradeLevel.Equals(1)))*@
											@foreach (var item in Model.Where(c => c.subject == "Filipino" && c.gradeLevel == "Kindergarten" && c.quarterNo == 1))
											{
												<tr>
													<td style="background-color:bisque; color:#3d4449;" href="@(item.files)" target="_blank" class="pb-2">
														@Html.DisplayFor(modelItem => item.moduleNo)
														@Html.DisplayFor(modelItem => item.title)

													</td>
													<td>
														@Html.DisplayFor(modelItem => item.description)
													</td>

													<td>
														<a href="@(item.files)" target="_blank" class="button mt-2">View <i class="fa fa-book" aria-hidden="true"></i></a>
													</td>
												</tr>
											}
											@foreach (var item in Model.Where(c => c.subject == "Filipino" && c.gradeLevel == "Kindergarten" && c.quarterNo == 2))
											{
												<tr>
													<td style="		background-color: #c4ecff; color:#3d4449;" href="@(item.files)" target="_blank" class="pb-2">
														@Html.DisplayFor(modelItem => item.moduleNo)
														@Html.DisplayFor(modelItem => item.title)

													</td>
													<td>
														@Html.DisplayFor(modelItem => item.description)
													</td>

													<td>
														<a href="@(item.files)" target="_blank" class="button mt-2">View <i class="fa fa-book" aria-hidden="true"></i></a>
													</td>
												</tr>
											}
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</li>
				</ul>
			</section>
		</div>
	</div>

	<!-- Sidebar -->
	<div id="sidebar">
		<div class="inner">


			<!-- Menu -->
			<nav id="menu">
				<header class="major">
					<h2>Menu</h2>
				</header>

				<ul>
					<li>@Html.ActionLink("General", "Library", "Home")</li>
					<li>

						<span class="opener">Web Directory</span>
