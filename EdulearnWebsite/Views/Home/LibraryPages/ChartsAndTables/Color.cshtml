
<link rel="stylesheet" href="~/wwwroot/css/library.css" />
<link href="https://fonts.googleapis.com/css?family=Muli:400,600" rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Bree+Serif&display=swap" rel="stylesheet">
<script src="https://kit.fontawesome.com/a076d05399.js"></script>
<style>
      body {
        cursor: pointer;
        font-family: "Bree Serif", serif;
        color: #424951;
      }
      .container {
        width: 100%;
        height: fit-content;
        margin: 10px auto;
      }
      .pri,
      .second,
      .inter,
      .basic,
      .neut {
        display: flex;
        flex-wrap: wrap;
        padding: 10px;
        box-sizing: border-box;
      }

      .container li {
        margin: 10px 5px;
        width: 200px;
      }
      img {
        width: 230px;
        height: 200px;
        margin-top: 10px;
        border-radius: 25px;
      }
      .container .clr {
        position: relative;
        box-shadow: 8px 10px 18px #615858;
        border: 4px solid #f3f3f3;
        padding: 1px 1px 1px;
        text-align: center;
        overflow: hidden;
        border-radius: 25px;
        list-style: none;
        border-radius: 25px;
      }
      .t {
        margin-top: 50px;
      }
      .header {
        border-radius: 30px;
        line-height: 55px;
        margin-top: 150px;
        font-size: 35px;
        background: 50px #f3f3f3;
        font-size: 32px;
      }
</style>
<!-- Wrapper -->
<div id="wrapper">
	<!-- Main -->
	<div id="main">
		<div class="inner">
			<!-- Header -->
			<header id="header">
				<a class="logo"><strong>Colors</strong></a>
				<div class="main-cont">
					<div id="time-i">
						<h1>-</h1>
					</div>
					<div id="date-i">
						<h1>-</h1>
					</div>
					<div id="day-i">
						<h1>-</h1>
					</div>
				</div>
			</header>

			<!-- Content -->
			<div class="container">
				<h2>Primary Colors</h2>
				<div class="pri">
					<li class="clr" style="background-color: yellow">
						<div class="header" id="yellow">Yellow</div>
					</li>
					<li class="clr" style="background-color: red">
						<div class="header" id="red">Red</div>
					</li>
					<li class="clr" style="background-color: blue">
						<div class="header" id="blue">Blue</div>
					</li>
				</div>
				<h2 class="t">Secondary Colors</h2>
				<div class="second">
					<li class="clr" style="background-color: orange">
						<div class="header" id="orange">Orange</div>
					</li>
					<li class="clr" style="background-color: violet">
						<div class="header" id="violet">Violet</div>
					</li>
					<li class="clr" style="background-color: green">
						<div class="header" id="green">Green</div>
					</li>
				</div>
				<h2 class="t">Tertiary Colors</h2>
				<div class="inter">
					<li class="clr" style="background-color: #ffae42">
						<div class="header" id="amber">Amber</div>
					</li>
					<li class="clr" style="background-color: #ff5349">
						<div class="header" id="vermilion">Vermilion</div>
					</li>
					<li class="clr" style="background-color: magenta">
						<div class="header" id="magenta">Magenta</div>
					</li>
					<li class="clr" style="background-color: indigo">
						<div class="header" id="indigo">Indigo</div>
					</li>
					<li class="clr" style="background-color: teal">
						<div class="header" id="teal">Teal</div>
					</li>
					<li class="clr" style="background-color: chartreuse">
						<div class="header" id="charts">Chartreuse</div>
					</li>
				</div>
				<h2 class="t">Neutral Colors</h2>
				<div class="neut">
					<li class="clr" style="background-color: white">
						<div class="header" id="white">White</div>
					</li>
					<li class="clr" style="background-color: beige">
						<div class="header" id="beige">Beige</div>
					</li>
					<li class="clr" style="background-color: tan">
						<div class="header" id="tan">Tan</div>
					</li>
					<li class="clr" style="background-color: #887464">
						<div class="header" id="taupe">Taupe</div>
					</li>
					<li class="clr" style="background-color: gray">
						<div class="header" id="gray">Gray</div>
					</li>
					<li class="clr" style="background-color: brown">
						<div class="header" id="brwn">Brown</div>
					</li>
					<li class="clr" style="background-color: black">
						<div class="header" id="black">Black</div>
					</li>
				</div>
			</div>
		</div>
	</div>

	<!-- Sidebar -->
	<div id="sidebar">
        <div class="inner">
            <!-- Home Back Button -->
            <a href="/Home">
                <div class="home-back-button">
                    <i class="fa fa-arrow-left  fa-fw" aria-hidden="true"></i>Back
                </div>
            </a>

            <!-- Menu -->
            <nav id="menu">
                <header class="major">
                    <h2>Menu</h2>
                </header>

                <ul>
                    <li>@Html.ActionLink("General", "Library", "Home")</li>
                    <li>

                        <span class="opener">Web Directory</span>
                        <ul>
                            <li>@Html.ActionLink("English", "EnglishD", "Home", null)</li>
                            <li>@Html.ActionLink("Math", "MathD", "Home", null)</li>
                            <li>@Html.ActionLink("Filipino", "FilipinoD", "Home", null)</li>
                            <li>@Html.ActionLink("Science", "ScienceD", "Home", null)</li>
                        </ul>
                    </li>
                    <li>
                        <span class="opener">Modules</span>
                        <ul>
                            <li>@Html.ActionLink("Kindergarten", "Kindergarten", "Home", null)</li>
                            <li>@Html.ActionLink("Elementary", "Elementary", "Home", null)</li>
                            <li>@Html.ActionLink("Junior High School", "Junior_high", "Home", null)</li>
                        </ul>
                    </li>
                    <li>
                        <span class="opener">Tables & Charts</span>
                        <ul>
                            <li>@Html.ActionLink("Flags", "Flags", "Home", null)</li>
                            <li>@Html.ActionLink("Numbers", "NumberChart", "Home", null)</li>
                            <li>@Html.ActionLink("Letters", "LetterChart", "Home", null)</li>
                            <li>@Html.ActionLink("Shapes", "Shapes", "Home", null)</li>
                            <li>@Html.ActionLink("Colors", "Color", "Home", null)</li>
                            <li>@Html.ActionLink("Body Parts", "BodyParts", "Home", null)</li>
                            <li>@Html.ActionLink("Multiplication Table", "MultiplicationTable", "Home", null)</li>
                        </ul>
                    </li>
                    <li>
                        <span class="opener">Maps</span>
                        <ul>
                            <li>@Html.ActionLink("World Map", "World", "Home", null)</li>
                            <li>@Html.ActionLink("Country Map", "Country", "Home", null)</li>
                            <li>@Html.ActionLink("Google Map", "Google", "Home", null)</li>
                        </ul>
                    </li>
                    <li>
                        <span class="opener">Videos</span>
                        <ul>
                            <li>@Html.ActionLink("Mini Youtube", "Youtube", "Home", null)</li>
                            <li>@Html.ActionLink("Chanel Recommendation", "Channels", "Home", null)</li>
                        </ul>
                    </li>
                    <li>
                        <span class="opener">eBooks</span>
                        <ul>
                            <li>@Html.ActionLink("Google Ebooks", "Ebook", "Home", null)</li>
                            <li>@Html.ActionLink("Ebooks Web Directory", "eBooksD", "Home", null)</li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("Trivia", "Trivia", "Home", null)</li>
                    <li>@Html.ActionLink("Dictionary", "Dictionary", "Home", null)</li>
                    <li>@Html.ActionLink("Study Tips", "StudyTips", "Home", null)</li>
                </ul>
            </nav>

            <!-- Section -->
            <!-- Footer -->
            <footer id="footer">
                <p class="copyright">&copy; HPM. All rights reserved. </p>
            </footer>
        </div>
	</div>
</div>

<!-- Library script -->
<script src="~/wwwroot/js/jquery.min.js"></script>
<script src="https://use.fontawesome.com/c0d25df00f.js"></script>
<script src="~/wwwroot/js/browser.min.js"></script>
<script src="~/wwwroot/js/breakpoints.min.js"></script>
<script src="~/wwwroot/js/util.js"></script>
<script src="~/wwwroot/js/library.js"></script>
<script>
	//style
	var style1 = {
		"color": "#F3F3F3", "font-size": "28px", "background": "#615858", "overflow": "hidden",
		"border-radius": "25px"
	}; //mouse over
	var style2 = {
		"color": "#615858", "font-size": "32px", "background": "#F3F3F3", "overflow": "hidden",
		"border-radius": "25px"
	}; //mouse out
	//end of style
	$(document).ready(function () {
		//primary colors
		$("#yellow").mouseover(function () { $("#yellow").text("Red + Light Green").css(style1); });
		$("#yellow").mouseout(function () { $("#yellow").text("Yellow").css(style2); });
		$("#red").mouseover(function () { $("#red").text("Magenta + Yellow").css(style1); });
		$("#red").mouseout(function () { $("#red").text("Red").css(style2); });
		$("#blue").mouseover(function () { $("#blue").text("Magenta + Aqua").css(style1); });
		$("#blue").mouseout(function () { $("#blue").text("Blue").css(style2); });
		//seconadry colors
		$("#orange").mouseover(function () { $("#orange").text("Red + Yellow").css(style1); });
		$("#orange").mouseout(function () { $("#orange").text("Orange").css(style2); });
		$("#violet").mouseover(function () { $("#violet").text("Red + Blue").css(style1); });
		$("#violet").mouseout(function () { $("#violet").text("Violet").css(style2); });
		$("#green").mouseover(function () { $("#green").text("Yellow + Blue").css(style1); });
		$("#green").mouseout(function () { $("#green").text("Green").css(style2); });
		//tertiary colors
		$("#amber").mouseover(function () { $("#amber").text("Yellow + Orange").css(style1); });
		$("#amber").mouseout(function () { $("#amber").text("Amber").css(style2); });
		$("#vermilion").mouseover(function () { $("#vermilion").text("Red + Orange").css(style1); });
		$("#vermilion").mouseout(function () { $("#vermilion").text("Vermilion").css(style2); });
		$("#magenta").mouseover(function () { $("#magenta").text("Red + Purple").css(style1); });
		$("#magenta").mouseout(function () { $("#magenta").text("Magenta").css(style2); });
		$("#teal").mouseover(function () { $("#teal").text("Blue + Green").css(style1); });
		$("#teal").mouseout(function () { $("#teal").text("Teal").css(style2); });
		$("#charts").mouseover(function () { $("#charts").text("Yellow + Green").css(style1); });
		$("#charts").mouseout(function () { $("#charts").text("Chartreuse").css(style2); });
		$("#indigo").mouseover(function () { $("#indigo").text("Blue + Violet").css(style1); });
		$("#indigo").mouseout(function () { $("#indigo").text("Indigo").css(style2); });
		//neutral colors
		$("#white").mouseover(function () { $("#white").text("White").css(style1); });
		$("#white").mouseout(function () { $("#white").text("White").css(style2); });
		$("#beige").mouseover(function () { $("#beige").text("Tan + White").css(style1); });
		$("#beige").mouseout(function () { $("#beige").text("Beige").css(style2); });
		$("#tan").mouseover(function () { $("#tan").text("Beige + Taupe").css(style1); });
		$("#tan").mouseout(function () { $("#tan").text("Tan").css(style2); });
		$("#taupe").mouseover(function () { $("#taupe").text("Tan + Gray").css(style1); });
		$("#taupe").mouseout(function () { $("#taupe").text("Taupe").css(style2); });
		$("#gray").mouseover(function () { $("#gray").text("White + Black").css(style1); });
		$("#gray").mouseout(function () { $("#gray").text("Gray").css(style2); });
		$("#brwn").mouseover(function () { $("#brwn").text("Blue + Orange").css(style1); });
		$("#brwn").mouseout(function () { $("#brwn").text("Brown").css(style2); });
		$("#black").mouseover(function () { $("#black").text("Dark Colors").css(style1); });
		$("#black").mouseout(function () { $("#black").text("Black").css(style2); });
	}); 
</script>
