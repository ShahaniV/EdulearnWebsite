		$("#green").mouseout(function () { $("#green").text("Green").css(style2); });
		//tertiary colors
		$("#amber").mouseover(function () { $("#amber").text("Yellow + Orange").css(style1); });
		$("#amber").mouseout(function () { $("#amber").text("Amber").css(style2); });
		$("#vermilion").mouseover(function () { $("#vermilion").text("Red + Orange").css(style1); });
		$("#vermilion").mouseout(function () { $("#vermilion").text("Vermilion").css(style2); });
		$("#magenta").mouseover(function () { $("#magenta").text("Red + Purple").css(style1); });
		$("#magenta").mouseout(function () { $("#magenta").text("Magenta").css(style2); });
		$("#teal").mouseover(function () { $("#teal").text("Blue + Green").css(style1); });
		$("#teal").mouseout(function () { $("#teal").text("Teal").css(style2); });
		$("#charts").mouseover(function () { $("#charts").text("Yellow + Green").css(style1); });
		$("#charts").mouseout(function () { $("#charts").text("Chartreuse").css(style2); });
		$("#indigo").mouseover(function () { $("#indigo").text("Blue + Violet").css(style1); });
		$("#indigo").mouseout(function () { $("#indigo").text("Indigo").css(style2); });
		//neutral colors
		$("#white").mouseover(function () { $("#white").text("White").css(style1); });
		$("#white").mouseout(function () { $("#white").text("White").css(style2); });
		$("#beige").mouseover(function () { $("#beige").text("Tan + White").css(style1); });
		$("#beige").mouseout(function () { $("#beige").text("Beige").css(style2); });
		$("#tan").mouseover(function () { $("#tan").text("Beige + Taupe").css(style1); });
		$("#tan").mouseout(function () { $("#tan").text("Tan").css(style2); });
		$("#taupe").mouseover(function () { $("#taupe").text("Tan + Gray").css(style1); });
		$("#taupe").mouseout(function () { $("#taupe").text("Taupe").css(style2); });
		$("#gray").mouseover(function () { $("#gray").text("White + Black").css(style1); });
		$("#gray").mouseout(function () { $("#gray").text("Gray").css(style2); });
		$("#brwn").mouseover(function () { $("#brwn").text("Blue + Orange").css(style1); });
		$("#brwn").mouseout(function () { $("#brwn").text("Brown").css(style2); });
		$("#black").mouseover(function () { $("#black").text("Dark Colors").css(style1); });
		$("#black").mouseout(function () { $("#black").text("Black").css(style2); });
	});
</script>
