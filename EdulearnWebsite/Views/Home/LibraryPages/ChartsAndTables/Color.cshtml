<style>
	body {
		cursor: pointer;
		font-family: "Bree Serif", serif;
		color: #424951;
	}

	.container {
		width: 100%;
		height: auto;
		margin: 10px auto;
	}

	.pri {
		display: flex;
		flex-wrap: wrap;
	}

	img {
		width: 280px;
		height: 250px;
		margin-top: 10px;
	}

	.container .clr {
		width: 260px;
		height: 250px;
		margin: 10px 10px 10px 0px;
		background-color: #F3F3F3;
		position: relative;
		box-shadow: 8px 10px 18px #615858;
		border: 2px solid #F3F3F3;
		padding: 10px 1px 1px;
		text-align: center;
		overflow: hidden;
		border-radius: 20px;
		list-style: none;
		border-radius: 20px;
	}

	.i2 {
		margin-top: 1px;
		font-size: 32px;
		margin-bottom: 0px;
	}

	.image__overlay {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background: rgba(0, 0, 0, 0.6);
		color: #F3F3F3;
		font-size: 32px;
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
		opacity: 0;
		transition: opacity 0.75s;
	}

	.image__overlay--blur {
		backdrop-filter: blur(90px);
	}

	.image__overlay > * {
		transform: translateY(20px);
		transition: transform 0.75s;
	}

	.image__overlay:hover {
		opacity: 1;
	}

		.image__overlay:hover > * {
			transform: translateY(0);
		}

	.color {
		font-size: 50px;
		-webkit-text-stroke: 2px #3c4146;
		-webkit-text-fill-color: #F3F3F3;
	}

	.mixed {
		font-size: 30px;
		margin-top: 3px;
		-webkit-text-stroke: 1.5px #3c4146;
		-webkit-text-fill-color: #F3F3F3;
	}

	.div-style {
		width: 220px;
		height: 190px;
		margin: 0 auto;
		text-align: center;
	}

	.image__overlay--primaryA {
		background: red;
	}

	.image__overlay--primaryB {
		background: yellow;
	}

	.image__overlay--primaryC {
		background: blue;
	}

	.image__overlay--primaryD {
		background: orange;
	}

	.image__overlay--primaryE {
		background: purple;
	}

	.image__overlay--primaryF {
		background: green;
	}

	.image__overlay--primaryG {
		background: #FACC2E;
	}

	.image__overlay--primaryH {
		background: #FF4000;
	}

	.image__overlay--primaryI {
		background: magenta;
	}

	.image__overlay--primaryJ {
		background: violet;
	}

	.image__overlay--primaryK {
		background: teal;
	}

	.image__overlay--primaryL {
		background: chartreuse;
	}

	.image__overlay--primaryM {
		background: white;
	}

	.image__overlay--primaryN {
		background: black;
	}

	.image__overlay--primaryO {
		background: #F5ECCE;
	}

	.image__overlay--primaryP {
		background: #61380B;
	}

	.image__overlay--primaryQ {
		background: #F3E2A9;
	}

	.image__overlay--primaryR {
		background: #C19A6B;
	}

	.image__overlay--primaryS {
		background: #F5F5DC;
	}

	.image__overlay--primaryT {
		background: #ADA96E;
	}

	.image__overlay--primaryU {
		background: #000080;
	}

	.image__overlay--primaryV {
		background: #34282C;
	}

	.image__overlay--primaryW {
		background: #493D26;
	}
</style>
